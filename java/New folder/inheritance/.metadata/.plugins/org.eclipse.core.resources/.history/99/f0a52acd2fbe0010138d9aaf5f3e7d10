package ss;

 class Studentt {
int fid;
String name;
int distance;
static int count=0;
 static int getCount() {
	return count;
}
public static void setCount(int count) {
	Student1.count = count;
}
public int getFid() {
	return fid;
}
public void setFid(int fid) {
	this.fid = fid;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getDistance() {
	return distance;
}
public void setDistance(int distance) {
	this.distance = distance;
}


Studentt(){
	count++;
	
}
Studentt(int i,String n,int d){
	this.fid=i;
	this.name=n;
	this.distance=d;
	count++;
	
}
}
 
 class PlacedStudent{
 
	 int fid;
	 String name;
	 int  Distance;
	 String cName;
	 String designation;
	public int getFid() {
		return fid;
	}
	public void setFid(int fid) {
		this.fid = fid;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getDistance() {
		return Distance;
	}
	public void setDistance(int distance) {
		Distance = distance;
	}
	public String getcName() {
		return cName;
	}
	public void setcName(String cName) {
		this.cName = cName;
	}
	public String getDesignation() {
		return designation;
	}
	public void setDesignation(String designation) {
		this.designation = designation;
	}
	 
	
	PlacedStudent() {
	System.out.println("constructor -- ");
	}
	PlacedStudent(int i,String n,int d, String cn,String dd){
		this.fid=i;
		this.name=n;
		this.Distance=d;
		this.cName=cn;
		this.designation=dd;
	 
 
 }
 }
 
 public class Studentt{
	 public static void main(String [] args) {
	 Student1 s=new Student1();
	 Student1 s2=new Student1(1,"sam",2);
	 System.out.println(Student1.count);
	 }
 }
